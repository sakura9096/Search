 <script>
morph.init = function() {
    if (!morph.initCalled) {
        morph.markPerformance('morph-init');
        morph.initCalled = true;

        requirejs.config({
            paths: {
                'morph/react'       : '//m.files.bbci.co.uk/releases/1124/vendor/react/react.min',
                'morph/es5-shim'    : '//m.files.bbci.co.uk/releases/1124/vendor/es5-shim/es5-shim.min',
                'morph/es5-sham'    : '//m.files.bbci.co.uk/releases/1124/vendor/es5-shim/es5-sham.min',
                'morph/libs/morph'  : '//m.files.bbci.co.uk/releases/1124/libs/morph/dist/bundle',
                'morph/requirejs'   : '//m.files.bbci.co.uk/releases/1124/vendor/morph-requirejs/index',
                'live-push'         : '//a.files.bbci.co.uk/live-push-handshaker/live/v2/live-push/live-push'
            },
            shim: {
                'morph/react': {
                    deps: ['morph/es5-shim', 'morph/es5-sham']
                },
                'morph/es5-sham': {
                    deps: ['morph/es5-shim']
                }
            }
        });
    }

    requirejs(
        ['morph/react', 'morph/requirejs'],
        function(React) {
            window.React = React;

            requirejs(['morph/libs/morph'], function() {

                if (!morph.runToInits) {
                    morph.markPerformance('morph-init-core-deps-complete');
                    morph.runToInits = true;
                }

                morph.toInit.bundles.forEach(function(bundle) {
                    bundle();
                });
                morph.toInit.bundles = [];
                
                morph.markPerformance('morph-init-bundles-complete');

                morph.toInit.viewModels.forEach(function(viewModel) {
                    viewModel();
                });
                morph.toInit.viewModels = [];

                morph.markPerformance('morph-init-payloads-complete');

                morph.toInit.registers.forEach(function(register) {
                    register();
                });
                morph.toInit.registers = [];

                morph.markPerformance('morph-init-registers-complete');
            });
        }
    );
};
</script> <script>if (!morph.manualInit) { morph.init(); } /* morph-live-scores */</script>  <script> if (window.morph && window.morph.init && !window.morph.manualInit) { window.morph.init(); } </script>     </body> </html>               